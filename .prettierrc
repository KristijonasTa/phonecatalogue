{
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": false,
	"semi": true,
	"singleQuote": true,
	"printWidth": 100,
	"bracketSpacing": true,
	"htmlWhitespaceSensitivity": "css",
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"jsxBracketSameLine": false,
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"endOfLine": "lf"
